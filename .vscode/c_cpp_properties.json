{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/src/vcpkg/installed/x64-linux-dynamic/include/**",
                "/usr/include/**"
            ],
            "defines": [],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-std=c++20"
            ],
            "compilerPath": "/usr/sbin/gcc"
        },
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/dev/vcpkg/installed/x64-windows/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "cl.exe",
            "cppStandard": "c++20",
            "compilerArgs": [
                "/Zc:__cplusplus",
                "/std:c++20"
            ],
            "intelliSenseMode": "windows-msvc-x64"
        }
    ],
    "version": 4
}